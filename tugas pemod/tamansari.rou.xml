<?xml version="1.0" encoding="iso-8859-1"?>

<!-- https://sumo.dlr.de/docs/Definition_of_Vehicles,_Vehicle_Types,_and_Routes.html -->

<routes>
	<!-- kendaraan yang bergerak dari selatan ke utara -->
    <vType id="normal_car" vClass="passenger" maxSpeed="40" speedFactor="0.9" speedDev="0.2" sigma="0.5" guiShape="passenger"/>
    <vType id="sporty_car" vClass="passenger" maxSpeed="60" speedFactor="1.3" speedDev="0.1" sigma="0.1" guiShape="passenger"/>
    <vType id="trailer" vClass="trailer"  maxSpeed="30" speedFactor="1.1" speedDev="0.1" guiShape="truck/trailer"/>
    <vType id="coach" vClass="coach"  maxSpeed="30" speedFactor="1." speedDev="0.1" />

    <flow id="normal" type="normal_car" begin="0" end="5000" number="5000" from="jalan#1" to="jalan#5" departPos="last" departLane="best" />
    <flow id="sporty" type="sporty_car" begin="0" end="5000" number="300" from="jalan#1" to="jalan#5" departPos="last" departLane="best" />
    <flow id="coach" type="coach" begin="0" end="5000" number="300" from="jalan#1" to="jalan#5" departPos="last" departLane="best" />
    <flow id="trailer" type="trailer" begin="0" end="5000" number="700" from="jalan#1" to="jalan#5" departPos="last" departLane="best" />

	<!-- kendaraan yang bergerak dari utara ke selatan. tanda minus diletakkan sebagai konsistensi -->
    <vType id="-normal_car" vClass="passenger" maxSpeed="40" speedFactor="0.9" speedDev="0.2" sigma="0.5" guiShape="passenger"/>
    <vType id="-sporty_car" vClass="passenger" maxSpeed="60" speedFactor="1.3" speedDev="0.1" sigma="0.1" guiShape="passenger"/>
    <vType id="-trailer" vClass="trailer"  maxSpeed="30" speedFactor="1.1" speedDev="0.1" guiShape="truck/trailer"/>
    <vType id="-coach" vClass="coach"  maxSpeed="30" speedFactor="1." speedDev="0.1" />

    <flow id="-normal" type="normal_car" begin="0" end="5000" number="5000" from="-jalan#5" to="-jalan#1" departPos="last" departLane="best"/>
    <flow id="-sporty" type="sporty_car" begin="0" end="5000" number="300" from="-jalan#5" to="-jalan#1" departPos="last" departLane="best" />
    <flow id="-coach" type="coach" begin="0" end="5000" number="300" from="-jalan#5" to="-jalan#1" departPos="last" departLane="best" />
    <flow id="-trailer" type="trailer" begin="0" end="5000" number="700" from="-jalan#5" to="-jalan#1" departPos="last" departLane="best" />
</routes>

